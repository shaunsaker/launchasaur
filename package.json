{
  "name": "launchasaur",
  "productName": "Launchasaur",
  "version": "0.0.10",
  "description": "Launch all the things with our amazing Launchjitsuâ„¢ ðŸ¥‹",
  "main": ".webpack/main",
  "scripts": {
    "lint": "npm-run-all --parallel lint:*",
    "lint:eslint": "eslint --ext [.ts,.tsx] .",
    "lint:prettier": "prettier -c \"**/*.{ts,tsx}\"",
    "lint:tsc": "tsc",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "deploy": "yarn ts-node ./scripts/deploy"
  },
  "keywords": [],
  "author": {
    "name": "Shaun Saker",
    "email": "sakershaun@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "Launchasaur"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/renderer/index.html",
                  "js": "./src/renderer/index.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "shaunsaker",
              "name": "launchasaur-releases"
            },
            "prerelease": true
          }
        }
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/plugin-webpack": "6.0.0-beta.54",
    "@electron-forge/publisher-github": "^6.0.0-beta.61",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/cross-spawn": "^6.0.2",
    "@types/d3": "^6.7.0",
    "@types/mousetrap": "^1.6.8",
    "@types/node": "^15.12.2",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-transition-group": "^4.4.1",
    "@types/redux-logger": "^3.0.8",
    "@types/styled-components": "^5.1.9",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "copy-webpack-plugin": "6.4.1",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "css-loader": "^4.2.1",
    "dotenv": "^10.0.0",
    "electron": "12.0.7",
    "electron-log": "^4.4.4",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^5.0.14",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "node-loader": "^1.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.0.2",
    "update-electron-app": "^2.0.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@sentry/electron": "^2.5.0",
    "@tippyjs/react": "^4.2.5",
    "connected-react-router": "^6.9.1",
    "d3": "^6.7.0",
    "electron-is-accelerator": "^0.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "fkill": "^7.1.1",
    "flatted": "^3.1.1",
    "history": "4.10.1",
    "mousetrap": "^1.6.5",
    "ps-list": "^7.2.0",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.2",
    "redux": "^4.1.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.3.0",
    "typesafe-actions": "4.4.2",
    "unique-names-generator": "^4.5.0",
    "use-hooks": "^2.0.0-rc.5",
    "uuid": "^8.3.2"
  },
  "build": "2"
}
